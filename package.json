{
  "name": "ankrcode-project",
  "version": "0.1.0",
  "private": true,
  "description": "AnkrCode - AI Coding Assistant for Bharat",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @ankr/ankrcode-core build",
    "build:rocketlang": "pnpm --filter @ankr/rocketlang build",
    "dev": "pnpm --filter @ankr/ankrcode-core dev",
    "start": "pnpm --filter @ankr/ankrcode-core start",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "rm -rf packages/*/dist packages/*/.turbo",
    "ankrcode": "node packages/ankrcode-core/dist/cli/index.js"
  },
  "keywords": [
    "ai",
    "cli",
    "coding-assistant",
    "india",
    "hindi",
    "indic",
    "voice",
    "rocketlang"
  ],
  "author": "ANKR Labs",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
