#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules/@ankr/vibecoding-tools/dist/node_modules:/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules/@ankr/vibecoding-tools/node_modules:/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules/@ankr/node_modules:/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules:/root/ankrcode-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules/@ankr/vibecoding-tools/dist/node_modules:/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules/@ankr/vibecoding-tools/node_modules:/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules/@ankr/node_modules:/root/ankrcode-project/node_modules/.pnpm/@ankr+vibecoding-tools@file+..+ankr-labs-nx+packages+vibecoding-tools_hono@4.11.4_zod@4.3.5/node_modules:/root/ankrcode-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@ankr/vibecoding-tools/dist/server.js" "$@"
else
  exec node  "$basedir/../@ankr/vibecoding-tools/dist/server.js" "$@"
fi
